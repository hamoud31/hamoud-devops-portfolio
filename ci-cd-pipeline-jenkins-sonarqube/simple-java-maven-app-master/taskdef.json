{
  "family": "java-app-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::046049643079:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "simple-java-app",
      "image": "046049643079.dkr.ecr.eu-west-3.amazonaws.com/simple-java-app:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
